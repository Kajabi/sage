<!-- Default -->
<%= render "examples/elements/checkbox/markup",
id: "c1",
label_text: "Checkbox",
checked: false,
disabled: false,
has_error: false
%>
<!-- Checked -->
<%= render "examples/elements/checkbox/markup",
id: "c2",
label_text: "Checked",
checked: true,
disabled: false,
has_error: false
%>
<!-- Disabled -->
<%= render "examples/elements/checkbox/markup",
id: "c3",
label_text: "Disabled",
checked: false,
disabled: true,
has_error: false
%>
<!-- Checked & Disabled -->
<%= render "examples/elements/checkbox/markup",
id: "c4",
label_text: "Checked and disabled",
checked: true,
disabled: true,
has_error: false
%>
<!-- Error -->
<%= render "examples/elements/checkbox/markup",
id: "c5",
label_text: "Error",
checked: false,
disabled: false,
has_error: true
%>
<!-- Checked & Error -->
<%= render "examples/elements/checkbox/markup",
id: "c6",
label_text: "Checked & Error",
checked: true,
disabled: false,
has_error: true
%>
