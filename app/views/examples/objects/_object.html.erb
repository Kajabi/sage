<% info_content = sage_objects.select { |object| object[:title] == @title }[0] %>
<% props_content = render partial: "examples/objects/#{@title}/props" %>
<% do_content = render partial: "examples/objects/#{@title}/rules_do" %>
<% dont_content = render partial: "examples/objects/#{@title}/rules_dont" %>

<%= render "examples/shared/heading",
  info_content: info_content %>

<%= render "examples/shared/quick_links",
  props_content: props_content,
  do_content: do_content,
  dont_content: dont_content %>

<%= render "examples/shared/preview",
  type: "object",
  title: @title %>

<% if info_content[:use_legacy_html_code_source] %>
  <%= render "examples/shared/html_code",
    type: "object",
    title: @title %>
<% else %>
  <%= render "examples/shared/sage_rails_code",
    type: "object",
    title: @title %>
<% end %>

<%= render "examples/shared/props",
  props_content: props_content %>

<%= render "examples/shared/rules",
  do_content: do_content,
  dont_content: dont_content %>
