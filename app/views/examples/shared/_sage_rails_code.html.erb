<div class="sage-panel">
  <div class="sage-panel__header">
    <h2 id="<%= title %>-example-code" class="t-sage-heading-4">Ruby Code</h2>
  </div>
  <div class="sage-panel__body">
    <div class="example__code">
      <button class="example__expand-btn" aria-expanded="false" aria-controls="example-code-<%= title %>">Expand this code snippet</button>
      <div id="example-code-<%= title %>" class="sage-code-snippet" tabindex="-1">
        <pre class="prettyprint"><code><%= File.read(File.join( Rails.root, "app/views/examples/objects", title, "_preview.html.erb")) %></code></pre>
      </div>
    </div>
  </div>
</div>
