<%
colors = [
  "draft",
  "info",
  "success",
  "warning",
  "danger"
]
%>
<% colors.each do | color | %>
  <%= render "examples/elements/label/markup",
  color: color
  %>
<% end %>
<br/>
<br/>
<% colors.each do | color | %>
  <%= render "examples/elements/label/markup",
  color: color,
  type: "subtle"
  %>
<% end %>
<br/>
<br/>
<% colors.each do | color | %>
  <%= render "examples/elements/label/markup",
  color: color,
  type: "bold"
  %>
<% end %>